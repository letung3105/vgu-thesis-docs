  shorttitle = {Deep Learning Methods for Forecasting {{COVID}}-19 Time-{{Series}} Data},
  author = {Zeroual, Abdelhafid and Harrou, Fouzi and Dairi, Abdelkader and Sun, Ying},
  date = {2020-11},
