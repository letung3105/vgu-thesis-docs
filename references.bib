@article{dengDynamicsDevelopmentCOVID192020,
  title = {Dynamics and {{Development}} of the {{COVID}}-19 {{Epidemic}} in the {{United States}}: A {{Compartmental Model Enhanced With Deep Learning Techniques}}},
  shorttitle = {Dynamics and {{Development}} of the {{COVID}}-19 {{Epidemic}} in the {{United States}}},
  author = {Deng, Qi},
